{
	"name": "@nivalis/template",
	"type": "module",
	"version": "0.1.0",
	"private": true,
	"license": "MIT",
	"packageManager": "pnpm@10.23.0",
	"publishConfig": {
		"access": "public"
	},
	"homepage": "https://github.com/nivalis-studio/template#readme",
	"repository": {
		"url": "git+https://github.com/nivalis-studio/template.git"
	},
	"scripts": {
		"dev": "bun run --watch src/index.tsx",
		"lint": "biome check",
		"lint:fix": "biome check --write",
		"prepare": "lefthook install",
		"ts": "tsc --noEmit"
	},
	"dependencies": {},
	"devDependencies": {
		"@biomejs/biome": "catalog:",
		"@commitlint/cli": "catalog:",
		"@commitlint/config-conventional": "catalog:",
		"@nivalis/biome-config": "catalog:",
		"@total-typescript/ts-reset": "catalog:",
		"@types/bun": "catalog:",
		"@types/node": "catalog:",
		"lefthook": "catalog:",
		"typescript": "catalog:",
		"zshy": "catalog:"
	},
	"zshy": "./src/index.ts",
	"files": ["dist"],
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.cts",
	"exports": {
		".": {
			"types": "./dist/index.d.cts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	}
}
